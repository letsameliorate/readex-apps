<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.cnd.makeproject</type>
    <configuration>
        <data xmlns="http://www.netbeans.org/ns/make-project/1">
            <name>BEM4I</name>
            <c-extensions/>
            <cpp-extensions>cpp</cpp-extensions>
            <header-extensions>h</header-extensions>
            <sourceEncoding>UTF-8</sourceEncoding>
            <make-dep-projects/>
            <sourceRootList/>
            <confList>
                <confElem>
                    <name>debug_gcc</name>
                    <type>1</type>
                </confElem>
                <confElem>
                    <name>debug_intel</name>
                    <type>1</type>
                </confElem>
                <confElem>
                    <name>debug_gcc_intel_tools</name>
                    <type>1</type>
                </confElem>
                <confElem>
                    <name>debug_intel_salomon</name>
                    <type>1</type>
                </confElem>
                <confElem>
                    <name>release_gcc</name>
                    <type>1</type>
                </confElem>
                <confElem>
                    <name>release_gcc_lib</name>
                    <type>2</type>
                </confElem>
                <confElem>
                    <name>release_intel</name>
                    <type>1</type>
                </confElem>
                <confElem>
                    <name>release_gcc_intel_tools</name>
                    <type>1</type>
                </confElem>
                <confElem>
                    <name>release_intel_salomon</name>
                    <type>1</type>
                </confElem>
                <confElem>
                    <name>release_intel_salomon_mic</name>
                    <type>1</type>
                </confElem>
                <confElem>
                    <name>release_intel_salomon_lib</name>
                    <type>2</type>
                </confElem>
            </confList>
            <formatting>
                <project-formatting-style>false</project-formatting-style>
            </formatting>
        </data>
    </configuration>
</project>
