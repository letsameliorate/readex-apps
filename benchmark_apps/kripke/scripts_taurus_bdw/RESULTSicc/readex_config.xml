        <metricPlugin>
            <!--<name>hdeem_sync_plugin</name>-->
            <name>x86_energy_sync_plugin</name>
        </metricPlugin>
        <metrics>
            <!--<node_energy>hdeem/BLADE/E</node_energy>-->
            <!--<cpu0_energy>hdeem/CPU0/E</cpu0_energy>-->
            <!--<cpu1_energy>hdeem/CPU1/E</cpu1_energy>-->
