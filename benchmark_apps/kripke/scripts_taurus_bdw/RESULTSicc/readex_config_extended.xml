        <frequency>
            <min_freq>1300</min_freq>
            <max_freq>2500</max_freq>
            <freq_step>200</freq_step>
            <default>2500</default>
        </frequency>
        <uncore>
            <min_freq>1200</min_freq>
            <max_freq>3000</max_freq>
            <freq_step>200</freq_step>
            <default>3000</default>
        </uncore>
    </tuningParameter>
