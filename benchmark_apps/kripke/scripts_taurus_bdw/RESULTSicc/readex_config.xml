            <max_freq>2400</max_freq>
            <freq_step>500</freq_step>
            <default>2500</default>
        </frequency>
        <uncore>
            <min_freq>1000</min_freq>
            <max_freq>3000</max_freq>
            <freq_step>1000</freq_step>
            <default>3000</default>
